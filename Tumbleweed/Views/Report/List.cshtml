@model System.Collections.Generic.IEnumerable<Tumbleweed.Models.WaitingListModel>
@{
    ViewBag.Title = "Welcome";
}


<div>

    <table class="table table-hover">
        <tr>
            <th>Email Address</th>
            <th>Time</th>
        </tr>

        @foreach(var listItem in Model)
        {

            <tr>
                <td>@listItem.Email</td>
                <td>@listItem.SignUpDate.DateTime.ToShortDateString() | @listItem.SignUpDate.DateTime.ToShortTimeString()</td>
            </tr>
        }

    </table>

</div>
